{
  "redirects": [
    {
      "source": "http://taxi-les-sables-olonne.fr/(.*)",
      "destination": "https://www.taxi-les-sables-olonne.fr/$1",
      "permanent": true
    },
    {
      "source": "https://taxi-les-sables-olonne.fr/(.*)",
      "destination": "https://www.taxi-les-sables-olonne.fr/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-Powered-By",
          "value": ""
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}